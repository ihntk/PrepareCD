<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.af.igor.prepcd.controller.MainFrameController">
   <top>
      <HBox alignment="CENTER_LEFT" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="machine" text="Machine" />
            <TextField fx:id="machineName" prefWidth="85.0" text="20" />
            <TextField fx:id="machineCode" layoutX="46.0" layoutY="10.0" prefWidth="70.0" />
            <Label fx:id="machineType" layoutX="10.0" layoutY="10.0" />
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="installButton" mnemonicParsing="false" text="Install" />
            <Button layoutX="548.0" layoutY="10.0" mnemonicParsing="false" text="Xls" fx:id="xlsButton" />
            <Button fx:id="machineButton" layoutX="548.0" layoutY="10.0" mnemonicParsing="false" text="Machine" />
            <Button fx:id="cdButton" layoutX="548.0" layoutY="10.0" mnemonicParsing="false" text="CD" />
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="2.0" top="2.0" />
         </BorderPane.margin>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="target" text="Target" />
            <Region>
               <HBox.margin>
                  <Insets left="2.0" right="2.0" />
               </HBox.margin>
               <padding>
                  <Insets left="5.0" right="5.0" />
               </padding>
            </Region>
            <Label fx:id="status" layoutX="12.0" layoutY="12.0" text="Status" />
         </children>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <ListView fx:id="machineDir" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <VBox alignment="CENTER" spacing="5.0">
                     <children>
                        <Button mnemonicParsing="false" text="&lt;-" />
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="-&gt;" />
                     </children>
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </VBox>
                  <ListView fx:id="remoteMachineDir" layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button mnemonicParsing="false" text="Button" />
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Button" />
                     </children>
                     <HBox.margin>
                        <Insets bottom="10.0" left="30.0" right="30.0" top="10.0" />
                     </HBox.margin>
                  </HBox>
                  <Region HBox.hgrow="ALWAYS" />
                  <HBox alignment="CENTER" layoutX="256.0" layoutY="20.0" spacing="5.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button mnemonicParsing="false" text="Button" />
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Button" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <ListView fx:id="basePlanDir" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <VBox alignment="CENTER" spacing="5.0">
                     <children>
                        <Button mnemonicParsing="false" text="&lt;-" />
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="-&gt;" />
                     </children>
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </VBox>
                  <ListView fx:id="cdDir" layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
